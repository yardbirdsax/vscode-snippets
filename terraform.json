{
	"ECS Container Definition": {
		"prefix": "ecscontainerdef",
		"body": [
			"{",
			"            \"cpu\": ${1:container cpu units},",
			"            \"environment\": [",
			"            ],            ",
			"            \"essential\": ${4|true,false|},",
			"            \"image\": \"${5:image name}\",",
			"            \"logConfiguration\": {",
			"                \"logDriver\": \"awslogs\",",
			"                \"options\": {",
			"                    \"awslogs-group\": \"/ecs/${6:log group name}\",",
			"                    \"awslogs-region\": \"${7:log group region}\",",
			"                    \"awslogs-stream-prefix\": \"ecs\"",
			"                }",
			"            },",
			"            \"memory\": ${8:container memory units},",
			"            \"mountPoints\": [],",
			"            \"name\": \"${9:container name}\",",
			"            \"portMappings\": [{",
			"                \"containerPort\": ${10:container port},",
			"                \"hostPort\": ${11:host port},",
			"                \"protocol\": \"${12:protocol}\"",
			"            }],",
			"            \"secrets\": [",
			"            ],",
			"            \"volumesFrom\": []",
			"        }"
		],
		"description": ""
	},
	"ECS Container Secret": {
		"prefix": "ecscontainersecret",
		"body": [
			"{",
			"    \"valueFrom\": \"${1:value from}\",",
			"    \"name\": \"${2:name}\"",
			"}"
		],
		"description": ""
	},
	"ECS Container Environment Variable": {
		"prefix": "ecscontainerenvvariable",
		"body": [
			"{",
			"    \"value\": \"${1:value}\",",
			"    \"name\": \"${2:name}\"",
			"}"
		],
		"description": ""
	},
	"AWS Role Policy": {
		"prefix": "awspolicy",
		"body": [
			"{",
			"  \"Version\":\"2012-10-17\",",
			"  \"Statement\":[",
			"    {",
			"      \"Effect\":\"Allow\",",
			"      \"Action\":[",
			"        \"ses:SendEmail\",",
			"        \"ses:SendRawEmail\"",
			"      ],",
			"      \"Resource\": [",
			"        \"arn:aws:ses:us-east-1:${data.aws_caller_identity.current.account_id}:identity/${var.verified_email_address}\"",
			"      ]",
			"    }",
			"  ]",
			"}"
		],
		"description": ""
	},
	"AWS Assume Role Policy": {
		"prefix": "awspolicyassumerole",
		"body": [
			"{",
			"  \"Version\": \"2012-10-17\",",
			"  \"Statement\": [",
			"      {",
			"      \"Action\": \"sts:AssumeRole\",",
			"      \"Principal\": {",
			"          \"Service\": [\"${1|codedeploy.amazonaws.com,ecs-tasks.amazonaws.com,ecs.amazonaws.com,ec2.amazonaws.com,lambda.amazonaws.com,events.amazonaws.com,logs.amazonaws.com,kinesis.amazonaws.com|}\"]",
			"      },",
			"      \"Effect\": \"Allow\",",
			"      \"Sid\": \"\"",
			"      }",
			"  ]",
			"}"
		],
		"description": ""
	},
	"awsregionnamevar": {
		"body": [
			"variable aws_region_name {",
			"  type = string",
			"  default = \"us-east-2\"",
			"  description = \"The AWS region where resources are deployed.\"",
			"}"
		]
	},
	"AWS Region Name variable block": {
		"prefix": "awsregionnamevar",
		"body": [
			"variable aws_region_name {",
			"  type = string",
			"  default = \"us-east-2\"",
			"  description = \"The AWS region where resources are deployed.\"",
			"}"
		],
		"description": ""
	}
}